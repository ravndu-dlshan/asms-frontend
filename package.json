{
  "name": "asms-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "docker:build": "docker build --build-arg NEXT_PUBLIC_BASE_URL=http://localhost:8080 -t asms-frontend .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:build-all": "docker-compose build",
    "docker:logs": "docker-compose logs -f",
    "docker:logs-frontend": "docker-compose logs -f frontend",
    "docker:logs-backend": "docker-compose logs -f backend",
    "docker:restart": "docker-compose restart",
    "docker:clean": "docker-compose down -v --rmi all"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.3.2",
    "axios": "^1.12.2",
    "js-cookie": "^3.0.5",
    "next": "15.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/js-cookie": "^3.0.6",
    "@types/next": "^8.0.7",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
